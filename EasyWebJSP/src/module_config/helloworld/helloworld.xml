<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	default-autowire="no">
	<bean id="helloWorldAction" class="org.easyweb.helloworld.action.HelloWorldAction" scope="prototype">
		<property name="helloWorldManager">
			<ref bean="helloWorldManager"/>
		</property>
	</bean>
	<bean id="helloWorldManager" class="org.easyweb.helloworld.manager.HelloWorldManager">
		<property name="helloWorldDao">
			<ref bean="helloWorldDao"/>
		</property>
	</bean>
	<bean id="helloWorldDao" class="org.easyweb.helloworld.dao.HelloWorldDao">
		<property name="sessionFactory">
			<ref bean="sessionFactory"/>
		</property>
	</bean>
	<bean id="helloWorldRPCManager" parent="baseJabSorb">
		<property name="serviceBean">
			<bean class="org.easyweb.helloworld.manager.HelloWorldManager"></bean>
		</property>
	</bean>
</beans>