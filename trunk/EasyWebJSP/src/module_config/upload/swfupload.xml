<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	default-autowire="no">
	<bean id="uploadAction" class="org.easyweb.commons.upload.action.UploadAction" scope="prototype">
		<property name="uploadManager">
			<ref bean="uploadManager"/>
		</property>
	</bean>
	<bean id="downLoadAction" class="org.easyweb.commons.upload.action.DownLoadAction" scope="prototype">
		<property name="uploadManager">
			<ref bean="uploadManager"/>
		</property>
	</bean>
	<bean id="uploadManager" class="org.easyweb.commons.upload.manager.UploadManager">
		<property name="uploadDao">
			<ref bean="uploadDao"/>
		</property>
	</bean>
	<bean id="uploadDao" class="org.easyweb.commons.upload.dao.UploadDao">
		<property name="sessionFactory">
			<ref bean="sessionFactory"/>
		</property>
	</bean>
</beans>